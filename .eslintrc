{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "parser": "babel-eslint",
  "rules": {
    "semi": "off",
    "quotes": "off",
    "linebreak-style": "off",
    "camelcase": "off",
    "indent": ["error", 4],
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "arrow-parens": "off"
  }
}